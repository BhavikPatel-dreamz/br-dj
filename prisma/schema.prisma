 generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model session {
  unique      Int       @unique(map: "unique_2") @default(autoincrement())
  id          String    @id
  shop        String
  state       String
  isOnline    Boolean   @default(false)
  scope       String?
  expires     DateTime?
  accessToken String
  userId      BigInt?

  @@index([unique], map: "unique")
}

model discountrules {
  id               Int       @id @default(autoincrement())
  title            String    @db.VarChar(255)
  description      String    @db.VarChar(255)
  type             String    @db.VarChar(255)
  active           Boolean   @default(true)
  createdAt        DateTime  @default(now())
  updatedAt        DateTime  @updatedAt
  startat          DateTime
  endat            DateTime?
  conditions       Json?
  selectproducts   Json?
  multiproduct     Json?
  selectcollection Json?
  discount         Json?
  varprodcheck     Json?
  products         Json?
  customcode       String?    @db.VarChar(255)
  appliedtype      String    @db.VarChar(255)
}

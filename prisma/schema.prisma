 generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model session {
  unique      Int       @unique(map: "unique_2") @default(autoincrement())
  id          String    @id
  shop        String
  state       String
  isOnline    Boolean   @default(false)
  scope       String?
  expires     DateTime?
  accessToken String
  userId      BigInt?
  firstName   String?
  lastName    String?
  email       String?
  accountOwner Boolean?
  locale      String?
  collaborator Boolean?
  emailVerified Boolean?
  
  @@index([unique], map: "unique")
}

